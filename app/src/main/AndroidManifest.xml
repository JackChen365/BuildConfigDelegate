<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.android.buildconfig.delegate.sample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity android:name=".BuildConfigTestActivity">
        </activity>
        <activity
            android:name=".BuildConfigFieldListActivity"
            android:label="BuildConfigFieldList">
        </activity>
        <activity
            android:name=".BuildConfigConfigurationActivity"
            android:label="BuildConfigConfiguration">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>